{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ],
    "curly": false,
    "one-line": false,
    "semicolon": false,
    "prefer-const": false,
    "triple-equals": false,
    "max-line-length": false,
    "no-inferrable-types": false,
    "no-trailing-whitespace": false,
    "space-before-function-paren": false
  }
}
